<table>
    <tbody>
    {{#notes}}
        <tr>
            <td width="96px">
                <img src="{{editor_figure}}" width="48px" height="48px">
                <div class="text-center">
                    <span>{{editor_name}}</span>
                </div>
            </td>
            <td width="100%" valign="top">
                <div class="row">
                    <span class="ml-auto"><small>{{edit_time}}</small></span>
                </div>
                <div>
                    <span>{{reply}}</span>
                </div>
            </td>
        </tr>
    {{/notes}}
    </tbody>
</table>

<div class="btn-group">
    {{#page_next}}
        <button id="note_page_next" class="btn btn-light">&or;</button>
    {{/page_next}}
</div>

